{"version": 2, "builds": [{"src": "package.json", "use": "@vercel/next"}], "routes": [{"src": "/(.*)", "dest": "/$1"}], "env": {"NEXT_PUBLIC_SITE_URL": "@site_url"}, "functions": {"app/api/**/*.js": {"runtime": "nodejs18.x"}}, "headers": [{"source": "/(.*)", "headers": [{"key": "X-Content-Type-Options", "value": "nosniff"}, {"key": "X-Frame-Options", "value": "DENY"}, {"key": "X-XSS-Protection", "value": "1; mode=block"}]}]}